
public class GooseInZooDivOne {
	
	final int MOD = 1000000007;
	int dist, M, N;
	String[] field;
	boolean[][] seen;
	
	boolean inside(int r, int c) {
		return r >= 0 && c >= 0 && r < M && c < N;
	}
	
	int dfs(int r, int c) {
		seen[r][c] = true;
		int ans = 1;
		for (int i = r - dist; i <= r + dist; ++i)
			for (int j = c - dist; j <= c + dist; ++j)
				if (inside(i, j) && field[i].charAt(j) == 'v' && 
						!seen[i][j] && Math.abs(i - r) + Math.abs(j - c) <= dist)
					ans += dfs(i, j);
		return ans;
	}

    public int count(String[] _field, int _dist) {
    	field = _field;
    	dist = _dist;
    	M = field.length;
    	N = field[0].length();
    	
    	long[][] C = new long[2501][2501];
    	for (int i = 0; i <= 2500; ++i)
    		C[i][0] = C[i][i] = 1;
    	
    	for (int i = 2; i <= 2500; ++i)
    		for (int j = 1; j < i; ++j)
    			C[i][j] = (C[i - 1][j] + C[i - 1][j - 1]) % MOD;

    	seen = new boolean[M][N];
    	int even = 0;
    	int odd = 0;
    	for (int i = 0; i < M; ++i)
    		for (int j = 0; j < N; ++j)
    			if (!seen[i][j] && field[i].charAt(j) == 'v') {
    				int c = dfs(i, j);
    				if (c % 2 == 0) ++even;
    				else ++odd;
    			}
    	long ans = 0;

    	for (int i = 0; i <= odd; i += 2)
    		for (int j = 0; j <= even; ++j)
	    		if (i + j > 0)
	    			ans = (ans + C[odd][i] * C[even][j]) % MOD;
    	return (int)ans;
        
    }

